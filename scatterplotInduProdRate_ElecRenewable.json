{
  "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
  "width": 600,
  "height": 300,
  "title": "COVID-19 Cases per Country (13 Oct 2020)",
  "data": {
  "url": "https://raw.githubusercontent.com/ElenaDoncila/Scatter_plot/main/Country_data.csv"
  },
  "mark": "circle",
  "encoding": {
  "y": {
  "field": "GDP-percapita(PPP)",
  "type": "quantitative",
  "title": "GDP - per capita",
  "axis":{"tickCount":7},
  "scale":{"type":"linear","domain": [0,100000]}
  },
  "x": {
  "field": "Electricity-fromotherrenewablesources",
  "type": "quantitative",
  "title": "% of Electricity Gained From Sustainable Sources",
  "scale":{"type":"linear","domain": [0,100]}
  },
  "color": {
  "field": "Continent",
  "type": "nominal"
  },
  "size": {
  "field": "Population",
  "type": "quantitative"
  }
  }
 }